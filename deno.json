{
  "name": "@totto/mastercss-daisyui",
  "version": "0.1.0",
  "lock": false,
  "exclude": [
    "./.history/*"
  ],
  "exports": {
    ".": "./mod.ts",
    "./theme": "./src/theme.ts",
    "./theme/*": "./src/*.ts",
    "./component": "./component.ts",
    "./component/*": "./component/*.ts",
    "./global": "./src/global.ts"
  },
  "tasks": {
    "demo": "deno run --watch --allow-read --allow-net demo/main.tsx",
    "precommit": "deno check *.ts && deno lint && deno fmt && deno test"
  },
  "imports": {
    "@master/css": "npm:@master/css@^2.0.0-rc.44",
    "culori": "npm:culori@^4.0.1"
  }
}
